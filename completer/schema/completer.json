{
  "title": "Notebook Completer",
  "description": "Notebook completer settings.",
  "jupyter.lab.shortcuts": [
    {
      "command": "completer:invoke-notebook",
      "keys": [
        "Tab"
      ],
      "selector": ".jp-Notebook.jp-mod-editMode .jp-mod-completer-enabled"
    }
  ],
  "properties": {
    "baseUrl": {
      "title": "TabNine Base URL",
      "type": "string",
      "description": "Jupyter TabNine Endpoint",
      "default": "http://localhost:8001"
    },
    "charLimit": {
      "title": "TabNine Request Character Limit",
      "type": "number",
      "description": "The maximum number of characters before and after current char to send to TabNine",
      "default": 10000
    },
    "maxResults": {
      "title": "TabNine Max Completion Results",
      "type": "number",
      "description": "The maximum number of results to return from TabNine",
      "default": 5
    }
  },
  "additionalProperties": false,
  "type": "object"
}